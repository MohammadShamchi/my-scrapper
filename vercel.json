{
  "version": 2,
  "name": "site2md",
  "builds": [
    {
      "src": "src/site2md/web/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/src/site2md/web/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/site2md/web/main.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}